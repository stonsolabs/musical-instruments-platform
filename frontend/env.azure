# Frontend Environment Variables for Azure Backend
# Copy this file to .env.local and update values as needed

# Azure Backend URL
NEXT_PUBLIC_API_BASE_URL=https://getyourmusicgear-api.azurewebsites.net

# API Key for backend authentication
API_KEY=your_api_key_here

# Production domain (for CORS and redirects)
NEXT_PUBLIC_DOMAIN=getyourmusicgear.com

# Environment
NODE_ENV=production
