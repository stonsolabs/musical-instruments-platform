{
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.getyourmusicgear.com"
        }
      ],
      "destination": "https://getyourmusicgear.com/:path*",
      "permanent": true
    }
  ]
}
